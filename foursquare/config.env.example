# Neo4j Configuration for Foursquare Import
# Copy this file to config.env and update with your settings

# Neo4j Connection String
# Format: bolt://username:password@host:port/database
NEO4J_CONNECTION_STRING=bolt://neo4j:password@localhost:7687/neo4j

# Alternative: Individual connection parameters
# NEO4J_URI=bolt://localhost:7687
# NEO4J_USERNAME=neo4j
# NEO4J_PASSWORD=password
# NEO4J_DATABASE=neo4j

# Import Settings
DATA_DIR=data
BATCH_SIZE=1000

# Optional: SSL/TLS Settings
# NEO4J_ENCRYPTED=true
# NEO4J_TRUST=TRUST_SYSTEM_CA_SIGNED_CERTIFICATES